#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--)
	{
	    int k,f=1,ii=0;
	    cin>>k;
	    for(int i=0;i<8;i++)
	    {
	        for(int j=0;j<8;j++)
	        {
	            if(ii==0)
	            {
	                cout<<"O";
	                ii++;
	                continue;
	            }
	            if(f<k)
	            {
	                cout<<".";
	                f++;
	                continue;
	            }
	            if(f>=k)
	            {
	                cout<<"X";
	                continue;
	            }
	        }
	        cout<<endl;
	    }
	}
	return 0;
}
